.lds-ellipsis div {
  position: absolute;
  top: 13px;
  width: 5px;
  height: 5px;
  border-radius: 50%;
  background: {{ font_col_base }};
  animation-timing-function: cubic-bezier(0, 1, 1, 0);
}

